          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        FirstName, LastName, CompanyName, City, Region, PostalCode, Phone, Extension
FROM            production.Suppliers AS S
